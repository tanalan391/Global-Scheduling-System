<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Login">
   <children>
      <GridPane layoutX="39.0" layoutY="20.0" prefHeight="352.0" prefWidth="505.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="180.0" minWidth="10.0" prefWidth="180.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="229.0" minWidth="10.0" prefWidth="178.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="158.0" minWidth="10.0" prefWidth="147.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="welcomeLabel" contentDisplay="CENTER" prefHeight="35.0" prefWidth="137.0" text="  Welcome!" GridPane.columnIndex="1">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label fx:id="userNameLabel" contentDisplay="RIGHT" prefHeight="27.0" prefWidth="178.0" text="Username" GridPane.rowIndex="1">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="passwordLabel" prefHeight="27.0" prefWidth="186.0" text="Password" GridPane.rowIndex="2">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button fx:id="loginButton" layoutX="25.0" layoutY="25.0" mnemonicParsing="false" onAction="#onLogin" prefHeight="51.0" prefWidth="175.0" text="Login" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <TextField fx:id="userName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <PasswordField fx:id="password" promptText="Case Sensitive!" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label fx:id="userZoneID" alignment="CENTER" text="Label" GridPane.columnIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
